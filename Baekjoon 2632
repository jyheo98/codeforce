Problem : https://www.acmicpc.net/problem/2632
Code
___________________________________________________

#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
using namespace std;

int pizzaA[1000002] = { 0, }, pizzaB[1000002] = { 0, };

int main(void)
{
	int target;
	int m, n;
	int i, j;
	

	cin >> target >> m >> n;
	
	vector<int> A(m), B(n);
	int sumA = 0;
	int sumB = 0;

	for (i = 0; i < m; i++)
	{
		cin >> A[i];
		sumA += A[i];
	}
	for (i = 0; i < n; i++)
	{
		cin >> B[i];
		sumB += B[i];
	}

	pizzaA[sumA]++;
	pizzaB[sumB]++;

	for (i = 1 ; i <= m-1 ; i++)
	{
		int curSum = 0;
		for (j = 0; j < i; j++) curSum += A[j];
		for (j = 0; j < m; j++)
		{
			pizzaA[curSum] ++;
			curSum = curSum - A[j] + A[(j + i) % m];
		}
	}
	for (i = 1; i <= n - 1; i++)
	{
		int curSum = 0;
		for (j = 0; j < i; j++) curSum += B[j];
		for (j = 0; j < n; j++)
		{
			pizzaB[curSum] ++;
			curSum = curSum - B[j] + B[(j + i) % n];
		}
	}
	int sum = 0;
	for (i = 1; i <= 1000000; i++)
	{
		if (target < i) break;
		if (pizzaA[i] > 0) sum += pizzaA[i] * pizzaB[target - i];
		if (i == target) sum += pizzaA[i] + pizzaB[i];
	}
	cout << sum;

	return 0;
}
